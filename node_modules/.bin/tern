#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_tern@0.23.0@tern/bin/tern" "$@"
  ret=$?
else 
  node  "$basedir/../_tern@0.23.0@tern/bin/tern" "$@"
  ret=$?
fi
exit $ret
